#!/bin/sh

# suckmerge:
#+ merge all the branches with main
#+ compile and install

git checkout main
git reset --hard origin/main &&
for branch in $(git for-each-ref --format='%(refname)' refs/heads/ | cut -d'/' -f3); do
	if [ "$branch" != "main" ]; then
		echo "$branch"
		git merge $branch -m $branch
	fi
done

# make && sudo make clean install
